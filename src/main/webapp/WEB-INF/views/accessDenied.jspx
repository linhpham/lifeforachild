<div xmlns:spring="http://www.springframework.org/tags" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fmt="http://java.sun.com/jsp/jstl/fmt" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">	
	<script type="text/javascript">dojo.require("dijit.TitlePane")</script>
    <div style="width: 550px" id="_access_denied_title">
    	<spring:message var="title" code="accessDenied.titlepane" />
    	<script type="text/javascript">Spring.addDecoration(new Spring.ElementDecoration({elementId : '_access_denied_title', widgetType : 'dijit.TitlePane', widgetAttrs : {title: '${title}'}})); </script>
		<spring:message code="accessDenied.message"/>
	</div>
</div>